{
  "private": true,
  "name": "@luxdefi/sites-mono",
  "engines": {
    "pnpm": ">=8"
  },
  "packageManager": "pnpm@8.11.0",
  "scripts": {
    "clean:nm": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "clean:nxt": "find . -name '.next' -type d -prune -exec rm -rf '{}' +",
    "clean:all": "pnpm clean:nm && pnpm clean:nxt && rm pnpm-lock.yaml"
  },
  "dependencies": {
    "@hanzo/auth": "2.3.2",
    "@hanzo/commerce": "4.9.0",
    "@hanzo/ui": "3.0.18",
    "@next/mdx": "^14.1.0",
    "firebase": "10.8.0",
    "lucide-react": "0.344.0",
    "mobx": "^6.12.0",
    "mobx-react-lite": "^4.0.5",
    "mobx-utils": "^6.0.8",
    "next": "14.1.3",
    "next-themes": "^0.2.1",
    "next-usequerystate": "^1.17.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.50.1",
    "request": "^2.88.2",
    "supports-color": "8.1.1",
    "zod": "3.21.4"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "18.2.64"
   }
  }
}