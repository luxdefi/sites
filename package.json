{
  "private": true,
  "name": "@luxfi/web",
  "version": "0.1.0",
  "engines": {
    "pnpm": ">=9"
  },
  "scripts": {
    "clean:nm": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "clean:nxt": "find . -name '.next' -type d -prune -exec rm -rf '{}' +",
    "clean:all": "pnpm clean:nm && pnpm clean:nxt && rm pnpm-lock.yaml"
  },
  "devDependencies": {
    "webpack-watch-files-plugin": "^1.2.1"
  },
  "//": "https://stackoverflow.com/questions/76591038/module-not-found-esm-packages-supports-color-need-to-be-imported-use-import",
  "pnpm": {
    "overrides": {
      "react": "^18.3.1",
      "react-dom": "^18.3.1",
      "@types/react": "^18.3.2",
      "@types/react-dom": "^18.3.0",
      "supports-color": "8.1.1",
      "@luxfi/core": "5.1.7",
      "@hanzo/auth": "2.4.11",
      "@hanzo/commerce": "7.0.15",
      "@hanzo/ui": "3.8.19",
      "react-hook-form": "7.51.4" 
    },
    "allowedDeprecatedVersions": {
      "request": "*"
    }
  }
}
